{
  "presets": ["next/babel"],
  "plugins": [
    "inline-dotenv",
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    "transform-define"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": {
              "modules": "commonjs"
            }
          }
        ]
      ]
    },
    "development" : {
      "compact": false
    }
  }
}